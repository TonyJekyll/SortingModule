onerror {resume}
quietly WaveActivateNextPane {} 0
add wave -noupdate /testbench/clk_i
add wave -noupdate /testbench/async_rst_i
add wave -noupdate /testbench/sop_i
add wave -noupdate /testbench/eop_i
