add wave -noupdate /testbench/val_i
add wave -noupdate /testbench/sop_o
add wave -noupdate /testbench/eop_o
add wave -noupdate -radix hexadecimal /testbench/data_o
add wave -noupdate /testbench/val_o
add wave -noupdate /testbench/busy_o
TreeUpdate [SetDefaultTree]
WaveRestoreCursors {{Cursor 1} {0 ns} 0}
quietly wave cursor active 0
configure wave -namecolwidth 150
configure wave -valuecolwidth 100
configure wave -justifyvalue left
